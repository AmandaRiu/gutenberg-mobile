  "homepage": "http://facebook.github.io/react-native/",
  "documentation_url": "https://facebook.github.io/react-native/docs/image",
  "license": "MIT",
  "authors": "Facebook, Inc. and its affiliates",
  "platforms": {
    "ios": "9.0",
    "tvos": "9.2"
  },
  "source": {
    "git": "https://github.com/facebook/react-native.git",
    "tag": "v0.62.0"
  },
  "source_files": "*.{m}",
